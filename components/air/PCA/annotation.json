{
  "type": "DataProcessing",
  "name": "PollenPCA",
  "label": "Pollen PCA",
  "description": "PCA Components for pollen time series data ",
  "license": "cc by-nc-nd 4.0",
  "version": "1.0.0",
  "dockerImage": "192.168.219.5:8098/enbic2lab/air/pollen_pca:1.0.0",
  "tags": ["enbic2lab-air", "cc by-nc-nd 4.0", "by-nc-nd", "processing"],
  "parameters": [
    {
      "name": "--filepath",
      "label": "File path",
      "description": "Input CSV file path.",
      "defaultValue": "{{.inputs.input.path}}",
      "readOnly": true,
      "type": "string"
    },
    {
      "name": "--output",
      "label": "Output file path",
      "description": "Input CSV file path.",
      "defaultValue":"/mnt/shared/pca.csv",
      "readOnly": true,
      "type": "string"
    },
    {
      "name": "--n-components",
      "label": "N-Components",
      "description": "N-Components.",
      "defaultValue":0.7,
      "type": "float"
    },
    {
      "name": "--delimiter",
      "label": "Delimiter",
      "description": "Delimiter used in CSV.",
      "defaultValue":";",
      "type": "string"
    },
    {
      "name": "--date-column",
      "label": "Date Column",
      "description": "Date column in CSV",
      "defaultValue":"fecha",
      "type": "string"
    },
    {
      "name": "--pollen-column",
      "label": "Pollen Column",
      "description": "Name of Pollen Column",
      "defaultValue":"Platanus",
      "type": "string"
    }
  ],
  "inputs": [
    {
      "name": "input",
      "label": "Input CSV file",
      "path": "/mnt/shared/lagged_time_series.csv",
      "type": "tabulardataset"
    }
  ],
  "outputs": [
    {
      "name": "pca_csv",
      "label": "Output CSV file PCA",
      "path": "/mnt/shared/pca.csv",
      "type": "tabulardataset"
    }
  ],
  "mainScriptPath": "main.py",
  "testPath": "test_main.py",
  "dependencies": [
    {
      "name": "python",
      "version": "3.11.5",
      "license": "PSF",
      "url": "https://www.python.org/"
    }
  ],
  "resources": {
    "cores": 1,
    "memory": 128,
    "gpuNeeded": false,
    "gpuMemory": null,
    "estimatedTimeInMin": 4
  },
