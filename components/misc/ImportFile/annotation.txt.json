{
  "type": "DataCollection",
  "name": "importTxtFile",
  "label": "Import Text File",
  "description": "Imports a .txt file from an online resource given its url.",
  "license": "cc by-nc-nd 4.0",
  "version": "1.1.0",
  "dockerImage": "192.168.219.5:8098/enbic2lab/misc/importfile:1.1.0",
  "tags": ["cc by-nc-nd 4.0", "by-nc-nd", "conversion"],
  "parameters": [
    {
      "name": "--url",
      "label": "URL",
      "description": "URL of the file",
      "type": "string"
    },
    {
      "name": "--query-string",
      "label": "Query string",
      "description": "Query string to be added to the URL",
      "defaultValue": null,
      "type": "string"
    },
    {
      "name": "--rename",
      "label": "Rename to",
      "description": "Rename the file to the given name",
      "defaultValue": "output.txt",
      "readOnly": true,
      "type": "string"
    },
    {
      "name": "--output",
      "label": "Output directory",
      "description": "Path to the output directory",
      "defaultValue": "/mnt/shared/",
      "readOnly": true,
      "type": "string"
    }
  ],
  "inputs": [],
  "outputs": [
    {
      "name": "output",
      "label": "Output file",
      "path": "/mnt/shared/output.txt",
      "type": "text"
    }
  ],
  "mainScriptPath": "main.py",
  "testPath": "test_main.py",
  "dependencies": [
    {
      "name": "python",
      "version": "3.11.5",
      "license": "PSF",
      "url": "https://www.python.org/"
    }
  ],
  "resources": {
    "cores": 1,
    "memory": 128,
    "gpuNeeded": false,
    "gpuMemory": null,
    "estimatedTimeInMin": 4
  },
